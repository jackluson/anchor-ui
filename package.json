{
  "name": "anchor-ui-vue",
  "version": "0.10.9",
  "description": "anchor ui toolkit",
  "keywords": [
    "vue",
    "element-ui",
    "ui"
  ],
  "homepage": "https://github.com/jackluson/anchor-ui",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jackluson/anchor-ui.git"
  },
  "bugs": {
    "url": "https://github.com/jackluson/anchor-ui/issues"
  },
  "author": "jackuson <18219112108@163.com>",
  "license": "MIT",
  "main": "build/anchor-ui.common.js",
  "browser": "build/anchor-ui.umd.min.js",
  "module": "build/anchor-ui.esm.js",
  "scripts": {
    "dev:all": "concurrently \"yarn docs:dev\" \"yarn docgen\"",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "docs:deploy": "sh ./script/deploy.sh",
    "docgen": "vue-docgen -w -c docgen.config.js",
    "build:pkg": "rollup -c",
    "build:watch": "rollup -c -w",
    "build:lib": "rollup -c -w --lib",
    "postversion": "git push --follow-tags"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@vuepress/plugin-back-to-top": "^1.8.2",
    "@vuepress/plugin-nprogress": "^1.8.2",
    "async-validator": "1.11.5",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "generate-source-map": "^0.0.5",
    "postcss": "^8.3.6",
    "rollup": "^2.55.1",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "5.1.6",
    "sass": "^1.26.3",
    "sass-loader": "^8.0.2",
    "typescript": "^4.1.2",
    "vue-docgen-cli": "^4.33.4",
    "vue-template-compiler": "^2.6.14",
    "vuepress": "^1.7.1",
    "vuepress-plugin-typescript": "^0.3.1",
    "yargs": "^17.0.1"
  },
  "dependencies": {
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "concurrently": "^6.2.0",
    "core-js": "^3.6.5",
    "markdown-it-container": "^2.0.0",
    "vue": "^2.6.12",
    "vue-class-component": "^7.2.3",
    "vue-property-decorator": "^8.5.0"
  },
  "peerDependencies": {
    "element-ui": "^2.15.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "src",
    "build"
  ],
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
