{
  "name": "aurora-ui",
  "version": "0.1.0",
  "description": "pea ui toolkit",
  "repository": "https://github.com/jackluson/aurora-ui.git",
  "author": "jackuson <18219112108@163.com>",
  "license": "MIT",
  "main": "build/pea-ui.umd.min.js",
  "scripts": {
    "dev:all": "concurrently \"yarn docs:dev\" \"yarn docgen\"",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "docgen": "vue-docgen -w -c docgen.config.js",
    "build:pkg": "vue-cli-service build --target lib --dest build --name pea-ui ./docs/.vuepress/addons/register.js",
    "build:watch": "vue-cli-service build --watch --target lib --dest docs/.vuepress/public/lib/pea-ui --name pea-ui ./docs/.vuepress/addons/register.js"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vuepress/plugin-back-to-top": "^1.8.2",
    "@vuepress/plugin-nprogress": "^1.8.2",
    "async-validator": "1.11.5",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "sass": "^1.26.3",
    "sass-loader": "^8.0.2",
    "typescript": "^4.1.2",
    "vue": "^2.6.12",
    "vue-docgen-cli": "^4.33.4",
    "vue-template-compiler": "^2.6.12",
    "vuepress": "^1.7.1",
    "vuepress-plugin-typescript": "^0.3.1",
    "yargs": "^15.3.1"
  },
  "dependencies": {
    "@jackluson/ui": "^0.1.0",
    "concurrently": "^6.2.0",
    "core-js": "^3.6.5",
    "element-ui": "^2.14.1",
    "markdown-it-container": "^2.0.0",
    "vue-class-component": "^7.2.3",
    "vue-property-decorator": "^8.5.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
